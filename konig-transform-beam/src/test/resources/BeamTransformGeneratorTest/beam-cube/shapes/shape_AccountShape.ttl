@prefix crm: <http://example.com/ns/crm/> .
@prefix gcp: <http://www.konig.io/ns/gcp/> .
@prefix konig: <http://www.konig.io/ns/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix shape: <http://example.com/shape/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

shape:AccountShape sh:property  [ 
		sh:path crm:accountType ; 
		sh:class crm:AccountType ; 
		sh:nodeKind sh:IRI ; 
		rdf:predicate crm:accountType ; 
		sh:minCount 1 ; 
		sh:maxCount 1
	 ]  ,  [ 
		sh:path crm:industry ; 
		sh:class crm:Industry ; 
		sh:nodeKind sh:IRI ; 
		rdf:predicate crm:industry ; 
		sh:minCount 1 ; 
		sh:maxCount 1 ]  ; 
	a sh:Shape ; 
	konig:shapeDataSource <gs://accountshape-${gcpBucketSuffix}> ; 
	sh:targetClass crm:Account ; 
	sh:nodeKind sh:IRI . 

<gs://accountshape-${gcpBucketSuffix}> gcp:name "accountshape-${gcpBucketSuffix}" ; 
	gcp:location "us" ; 
	gcp:projectId "${gcpProjectId}" ; 
	gcp:storageClass "multi_regional" ; 
	a konig:GoogleCloudStorageBucket , konig:DataSource . 