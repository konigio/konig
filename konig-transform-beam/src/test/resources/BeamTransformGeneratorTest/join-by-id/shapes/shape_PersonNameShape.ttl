@prefix gcp: <http://www.konig.io/ns/gcp/> .
@prefix konig: <http://www.konig.io/ns/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix shape: <http://example.com/shapes/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

shape:PersonNameShape sh:property  [ 
		sh:path schema:givenName ; 
		sh:maxCount 1 ; 
		sh:minCount 0 ; 
		sh:datatype xsd:string ; 
		rdf:predicate schema:givenName ]  ; 
	a sh:Shape ; 
	konig:shapeDataSource <https://www.googleapis.com/bigquery/v2/projects/${gcpProjectId}/datasets/schema/tables/Person> , <gs://personnameshape-${gcpBucketSuffix}> ; 
	sh:nodeKind sh:IRI ; 
	sh:targetClass schema:Person . 

<https://www.googleapis.com/bigquery/v2/projects/${gcpProjectId}/datasets/schema/tables/Person> gcp:tableReference  [ 
		gcp:tableId "Person" ; 
		gcp:datasetId "schema" ; 
		gcp:projectId "${gcpProjectId}"
	 ]  ; 
	gcp:externalDataConfiguration  [ 
		gcp:csvOptions  [ 
			gcp:skipLeadingRows "1"^^xsd:long ]  ; 
		gcp:sourceUris "gs://personnameshape-${gcpBucketSuffix}/*" ; 
		gcp:sourceFormat "CSV" ]  ; 
	a konig:GoogleBigQueryTable , konig:DataSource . 

<gs://personnameshape-${gcpBucketSuffix}> gcp:name "personnameshape-${gcpBucketSuffix}" ; 
	gcp:location "us" ; 
	gcp:projectId "${gcpProjectId}" ; 
	gcp:storageClass "multi_regional" ; 
	gcp:notificationInfo  [ 
		gcp:eventTypes "${eventType}" ; 
		gcp:topic "${topicName}" ]  ; 
	a konig:GoogleCloudStorageBucket , konig:DataSource . 