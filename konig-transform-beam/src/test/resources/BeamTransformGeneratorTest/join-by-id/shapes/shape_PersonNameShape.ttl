@prefix gcp: <http://www.konig.io/ns/gcp/> .
@prefix konig: <http://www.konig.io/ns/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix shape: <http://example.com/shapes/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

shape:PersonNameShape sh:property  [ 
		sh:path schema:givenName ; 
		rdf:predicate schema:givenName ; 
		sh:datatype xsd:string ; 
		sh:maxCount 1 ; 
		sh:minCount 0 ]  ; 
	a sh:Shape ; 
	sh:nodeKind sh:IRI ; 
	sh:targetClass schema:Person ; 
	konig:shapeDataSource <gs://personnameshape-${gcpBucketSuffix}> . 

<gs://personnameshape-${gcpBucketSuffix}> gcp:name "personnameshape-${gcpBucketSuffix}" ; 
	gcp:location "us" ; 
	gcp:projectId "${gcpProjectId}" ; 
	gcp:storageClass "multi_regional" ; 
	a konig:GoogleCloudStorageBucket , konig:DataSource . 