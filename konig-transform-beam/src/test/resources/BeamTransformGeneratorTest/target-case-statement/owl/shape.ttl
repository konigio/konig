@prefix konig: <http://www.konig.io/ns/core/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix shape: <http://example.com/ns/shape/> .
@prefix vann: <http://purl.org/vocab/vann/> .

shape: a owl:Ontology ; 
	vann:preferredNamespacePrefix "shape" ; 
	rdfs:label "Shape Ontology" . 

shape:AnimalSourceShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.animal" . 

shape:AnimalTargetShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.animal" . 

shape:EnumShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.enumshape" . 

shape:PersonSourceShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.personsourceshape" . 