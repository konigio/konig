@prefix konig: <http://www.konig.io/ns/core/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix shape: <http://example.com/ns/shape/> .
@prefix vann: <http://purl.org/vocab/vann/> .

shape: a owl:Ontology ; 
	vann:preferredNamespacePrefix "shape" ; 
	rdfs:label "Shape Ontology" . 

shape:FirstNameIdentityShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.firstnameidentityshape" . 

shape:FirstNameSourceShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.person" . 

shape:IdentityShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.identityshape" . 

shape:LastNameIdentityShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.lastnameidentityshape" . 

shape:LastNameSourceShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.person" . 

shape:PersonTargetShape konig:mediaTypeBaseName "application/vnd.example.ns.shape.person" . 