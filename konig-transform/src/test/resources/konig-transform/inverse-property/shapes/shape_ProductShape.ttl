shape:ProductShape a sh:Shape ; 
	prov:wasGeneratedBy <http://www.konig.io/activity/XlBl2qL9kqUAAAFi_TqDlw> ; 
	sh:targetClass schema:Product ; 
	sh:nodeKind sh:IRI ; 
	sh:property  [ 
		sh:path schema:name ; 
		sh:datatype xsd:string ; 
		sh:minCount 1 ; 
		sh:maxCount 1
	 ]  ,  [ 
		sh:path x:ownerName ; 
		sh:datatype xsd:string ; 
		sh:minCount 1 ; 
		sh:maxCount 1 ; 
		konig:formula '''@context {
   "owns" : "http://schema.org/owns",
   "name" : "http://schema.org/name"
}
^owns.name''' ]  ; 
	konig:shapeDataSource <https://www.googleapis.com/bigquery/v2/projects/${gcpProjectId}/datasets/schema/tables/Product> . 

<http://www.konig.io/activity/XlBl2qL9kqUAAAFi_TqDlw> a konig:LoadModelFromSpreadsheet ; 
	as:endTime "2018-04-25T10:34:53.359-04:00"^^xsd:dateTime . 

<https://www.googleapis.com/bigquery/v2/projects/${gcpProjectId}/datasets/schema/tables/Product> a konig:GoogleBigQueryTable , konig:DataSource ; 
	gcp:tableReference  [ 
		gcp:projectId "${gcpProjectId}" ; 
		gcp:datasetId "schema" ; 
		gcp:tableId "Product" ]  . 